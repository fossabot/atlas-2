{
  "name": "atlas",
  "version": "0.0.0-development",
  "description": "Mapping library for jobs",
  "homepage": "https://github.com/chronark/atlas",
  "bugs": {
    "url": "https://github.com/chronark/atlas/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chronark/atlas.git"
  },
  "license": "MIT",
  "author": "Andreas Thomas <andreas.thomas@th-nuernberg.de>",
  "scripts": {
    "build": "webpack --mode=production --progress",
    "buildandserve": "yarn build && yarn serve",
    "dev": "webpack-dev-server --progress",
    "e2e": " node ./__tests__/e2e/testcafe.js ",
    "e2e:ci": "export CI=true || set CI=true && yarn e2e ",
    "e2e:ie": "export IE_ONLY=true || set IE_ONLY=true && yarn e2e",
    "format": "yarn prettier && yarn lint",
    "prettier": "prettier --write ./**/*.{js,jsx,ts,tsx,json} --loglevel warn",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "serve": "serve ./dist",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "start-server-and-test buildandserve http://localhost:5000 e2e",
    "test:e2e:ci": "start-server-and-test serve http://localhost:5000 e2e:ci",
    "test:e2e:ie": "start-server-and-test serve http://localhost:5000 e2e:ie",
    "test:unit": "jest",
    "tsc": "./node_modules/.bin/tsc --noEmit",
    "docker:build": "docker build -t chronark/atlas .",
    "docker:run": "docker run -it --rm -p 5000:5000 --name atlas atlas",
    "docker": "yarn docker:build && yarn docker:run",
    "docs:buildTypes": "yarn typedoc",
    "docs:show": "yarn docs:buildTypes && cd docs && yarn start",
    "docs:build": "yarn docs:buildTypes && cd docs && yarn build",
    "test:full": "yarn install && yarn format && yarn tsc && yarn test:unit && yarn build && yarn test:e2e:ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:unit"
    }
  },
  "dependencies": {
    "axios": "0.19.2",
    "logrocket": "1.0.6",
    "ol": "6.1.1",
    "ol-ext": "3.1.8",
    "ol-mapbox-style": "5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.3",
    "@babel/plugin-transform-for-of": "7.8.3",
    "@babel/plugin-transform-runtime": "7.8.3",
    "@babel/polyfill": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@babel/preset-typescript": "7.8.3",
    "@fullhuman/postcss-purgecss": "1.3.0",
    "@types/html-webpack-plugin": "3.2.1",
    "@types/jest": "25.1.0",
    "@types/node": "12.12.26",
    "@types/ol": "6.1.0",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-redux": "7.1.5",
    "@types/react-test-renderer": "16.9.2",
    "@typescript-eslint/eslint-plugin": "2.19.0",
    "@typescript-eslint/parser": "2.19.0",
    "babel-loader": "8.0.6",
    "babel-plugin-macros": "2.8.0",
    "copy-webpack-plugin": "5.1.1",
    "core-js": "3.6.4",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-airbnb-typescript": "6.3.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-standard": "14.1.0",
    "eslint-formatter-pretty": "3.0.1",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-jest": "23.6.0",
    "eslint-plugin-jsdoc": "18.11.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.17.0",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-testcafe": "0.2.1",
    "eslint-plugin-unicorn": "13.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-canvas-mock": "2.2.0",
    "lint-staged": "10.0.5",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "purgecss-webpack-plugin": "2.0.5",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-redux": "7.1.3",
    "react-test-renderer": "16.12.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
