{
  "name": "atlas",
  "version": "0.0.0-development",
  "description": "Mapping library for jobs",
  "homepage": "https://github.com/chronark/atlas",
  "bugs": {
    "url": "https://github.com/chronark/atlas/issues"
  },
  "repository": {
    "type": "git",
