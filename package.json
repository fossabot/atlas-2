    "serve": "11.3.0",
    "source-map-loader": "0.2.4",
    "start-server-and-test": "1.10.6",
    "style-loader": "1.1.3",
    "testcafe": "1.7.1",
    "testcafe-react-selectors": "4.0.0",
    "ts-jest": "25.1.0",
    "ts-loader": "6.2.1",
    "typedoc": "0.15.8",
    "typedoc-plugin-markdown": "2.2.16",
    "typescript": "3.7.5",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.10.1"
  }
}
