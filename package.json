{
  "name": "atlas",
  "version": "0.0.0-development",
  "description": "Mapping library for jobs",
  "homepage": "https://github.com/chronark/atlas",
  "bugs": {
    "url": "https://github.com/chronark/atlas/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chronark/atlas.git"
  },
  "license": "MIT",
  "author": "Andreas Thomas <andreas.thomas@th-nuernberg.de>",
  "scripts": {
    "build": "webpack --mode=production --progress",
    "buildandserve": "yarn build && yarn serve",
    "dev": "webpack-dev-server --progress",
    "e2e": " node ./__tests__/e2e/testcafe.js ",
    "e2e:ci": "export CI=true || set CI=true && yarn e2e ",
    "e2e:ie": "export IE_ONLY=true || set IE_ONLY=true && yarn e2e",
    "format": "yarn prettier && yarn lint",
    "prettier": "prettier --write ./**/*.{js,jsx,ts,tsx,json} --loglevel warn",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "serve": "serve ./dist",
    "test": "yarn test:unit && yarn test:e2e",
    "test:e2e": "start-server-and-test buildandserve http://localhost:5000 e2e",
    "test:e2e:ci": "start-server-and-test serve http://localhost:5000 e2e:ci",
    "test:e2e:ie": "start-server-and-test serve http://localhost:5000 e2e:ie",
    "test:unit": "jest",
    "tsc": "./node_modules/.bin/tsc --noEmit",
    "docker:build": "docker build -t atlas .",
    "docker:run": "docker run -it --rm -p 5000:5000 --name atlas atlas",
    "docker": "yarn docker:build && yarn docker:run",
    "docs": "yarn jsdoc -c jsdoc.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn docs && lint-staged",
      "pre-push": "yarn test"
    }
  },
  "dependencies": {
    "@sentry/browser": "^5.9.1",
    "axios": "^0.19.0",
    "ol": "^6.1.1",
    "ol-ext": "^3.1.6",
    "ol-mapbox-style": "^5.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/plugin-transform-for-of": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@fullhuman/postcss-purgecss": "^1.3.0",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/ol": "^5.3.6",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-macros": "^2.8.0",
    "better-docs": "^1.4.7",
    "copy-webpack-plugin": "^5.0.5",
    "core-js": "3.4.1",
    "css-loader": "^3.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-typescript": "^6.3.0",
    "eslint-config-prettier": "^6.6.0",
    "eslint-config-standard": "^14.0.1",
    "eslint-formatter-pretty": "^3.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^23.0.4",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-unicorn": "^13.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.7.0",
    "jest-canvas-mock": "^2.2.0",
    "jsdoc": "^3.6.3",
    "lint-staged": "^9.4.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "purgecss-webpack-plugin": "^1.6.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
